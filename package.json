{
	"name": "minesweeper",
	"version": "0.1.0",
	"homepage": "http://newesters.github.io/minesweeper_react",
	"private": true,
	"dependencies": {
		"bem-cn": "^3.0.1",
		"immutable": "^4.0.0-rc.12",
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	},
	"scripts": {
		"start": "parcel src/index.html",
		"build": "parcel build src/index.html -d build",
		"predeploy": "parcel build src/index.html -d build --public-url /minesweeper_react --no-content-hash",
		"deploy": "gh-pages -d build"
	},
	"devDependencies": {
		"@types/react": "^16.9.43",
		"@types/react-dom": "^16.9.8",
		"autoprefixer": "^9.8.5",
		"gh-pages": "^2.0.1",
		"parcel-bundler": "^1.12.4",
		"sass": "^1.26.10",
		"typescript": "^3.9.7"
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {
				"overrideBrowserslist": [
					"> 1%",
					"last 4 versions",
					"ie >= 9"
				]
			}
		}
	}
}
